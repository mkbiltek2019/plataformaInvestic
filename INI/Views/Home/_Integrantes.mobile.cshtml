    <div data-role="collapsible">
        <h4>Coordinación General</h4>
        @foreach (var item in Model.Where(m => m.tblEquipo_ID == 10011))
        {
            <address>
                <strong>@Html.DisplayFor(modelItem => item.int_nombre) @Html.DisplayFor(modelItem => item.int_apellido)</strong><br />
