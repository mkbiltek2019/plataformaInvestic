@model IEnumerable<INI.Models.DataBase.tblIntegrante>

<div class="titulosseccion" style="text-align: center;color:#33691E;top: 20%;position: fixed;left: 35%;font-size: 39px;">Nuestro Equipo de Trabajo</div>
