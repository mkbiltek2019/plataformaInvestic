@*@using datatabletes.Infrastructure*@

@model DateTime?


@Html.AddScriptFile("datetimepicker_plugin", "~/Scripts/jquery.datetimepicker.js")
@Html.AddScript("init_Date_Editor"
    , @"    $(function () {
        $('.datepicker').datetimepicker({
            lang: 'es',
            format: 'd/m/Y',
            formatDate: 'd/m/Y',
            dayOfWeekStart: 1,
            mask: true,
            timepicker: false
        });
    });")

@Html.TextBoxFor(m => m, "dd/MM/yyyy", new { @class = "datepicker" })




